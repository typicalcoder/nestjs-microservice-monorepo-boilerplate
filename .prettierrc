{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "printWidth": 80,
  "plugins": [
    "prettier-plugin-sort-json",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^@bootstrap/(.*)$",
    "",
    "^@microservice/(.*)$",
    "",
    "^@(.*)/(.*)$",
    "",
    "^[./]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "importOrderTypeScriptVersion": "5.0.0",
  "importOrderCaseSensitive": false
}